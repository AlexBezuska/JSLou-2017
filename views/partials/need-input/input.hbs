{{#ifEqual this.type "input"}}
<input
type="{{this.templateOptions.type}}"
class="form-control"
id="this.key"
name="{{this.key}}"
placeholder="{{this.templateOptions.placeholder}}"
value="{{this.data}}"
{{#if this.templateOptions.required}}required{{/if}} {{#if this.templateOptions.readonly}}readonly{{/if}} />
{{/ifEqual}}